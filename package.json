{
  "name": "starter-project",
  "version": "1.0.0",
  "author": "jmahecha@hugeinc.com",
  "license": "ISC",
  "scripts": {
    "build": "webpack --env NODE_ENV=production --config webpack/webpack.config.prod.js --color",
    "build:dev": "webpack --config webpack/webpack.config.dev.js --color --progress",
    "start": "webpack --watch --config webpack/webpack.config.dev.js --color --progress"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "modules": true
      }
    },
    "rules": {
      "comma-dangle": 0,
      "no-multi-spaces": 0,
      "semi": [
        "error",
        "always"
      ],
      "space-before-function-paren": [
        "error",
        "never"
      ],
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 3
        }
      ],
      "object-curly-spacing": [
        "error",
        "always",
        {
          "objectsInObjects": false,
          "arraysInObjects": false
        }
      ],
      "arrow-parens": 0,
      "max-len": [
        "error",
        200
      ],
      "no-unused-vars": [
        "error",
        {
          "args": "all"
        }
      ],
      "dot-location": 0,
      "quotes": [
        2,
        "single"
      ],
      "key-spacing": [
        "error",
        {
          "mode": "minimum"
        }
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.16.8",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint-config-google": "^0.14.0",
    "eslint-webpack-plugin": "^3.1.1",
    "eslint": "^8.14.0",
    "file-loader": "^6.2.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.1.0",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.2.3",
    "sass-loader": "^12.4.0",
    "sass": "~1.39.2",
    "style-loader": "^3.3.1",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-webpack-plugin": "^3.1.1",
    "stylelint": "^14.8.2",
    "ts-loader": "^8.0.14",
    "typescript": "^4.6.4",
    "watch": "^1.0.2",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.3",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-merge": "^5.8.0",
    "webpack": "^5.72.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "core-js": "^3.20.3"
  }
}
